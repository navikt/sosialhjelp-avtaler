{
  "name": "sosialhjelp-avtaler-server",
  "private": true,
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev": "cross-env NAIS_CLUSTER_NAME=mock-gcp USE_MSW=true ts-node-dev --transpile-only -r source-map-support/register -r dotenv/config src/server.ts",
    "test": "cross-env NAIS_CLUSTER_NAME=mock-gcp USE_MSW=true jest ./test",
    "build": "tsc -p .",
    "start": "cross-env NAIS_CLUSTER_NAME=mock-gcp USE_MSW=true node -r source-map-support/register -r dotenv/config dist/server.js",
    "clean": "rimraf dist",
    "prettier": "prettier -w src"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.3.1",
    "@navikt/nav-dekoratoren-moduler": "^1.6.9",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-http-proxy": "^1.6.3",
    "http-proxy-middleware": "^2.0.6",
    "jose": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "mustache-express": "^1.3.2",
    "node-jose": "^2.1.1",
    "openid-client": "^5.1.9",
    "prom-client": "^14.1.0",
    "source-map-support": "^0.5.21",
    "ulid": "^2.3.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.3",
    "@types/jest": "^28.1.8",
