    "@types/jsonwebtoken": "^8.5.9",
    "@types/mustache-express": "^1.2.2",
    "@types/node": "^18.7.13",
    "@types/node-jose": "^1.1.10",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "jest": "^28.1.3",
    "mammoth": "^1.4.21",
    "nock": "^13.2.9",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  }
